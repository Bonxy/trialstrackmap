{{#extend "default"}}
    {{#content "title-override"}}true{{/content}}
    {{#content "title" mode="prepend"}}
        Garage
    {{/content}}

    {{#content "styles" mode="append"}}
        <link rel="stylesheet" type="text/css" href="../css/style-garage.css{{cacheBuster}}" />
    {{/content}}

    {{#content "body"}}
        <div ng-init="pageReady = false" ng-hide="pageReady" class="loader spin"></div>
        <div class="page-body iss-loading" data-event="data:loaded" ng-class="{'is-ready':pageReady}">
            {{> navigation}}

            <div class="garage">
                <img class="paintjob" ng-src="//i.imgur.com/{{paintjobs.armadillo.[0].src}}" />
                <img class="background" ng-src="//i.imgur.com/{{paintjobs.garage.src}}" ng-load="pageReady = true" />
            </div>
        </div>
    {{/content}}
{{/extend}}